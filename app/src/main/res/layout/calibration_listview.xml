<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="36px"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36px"
        android:paddingLeft="5px"
        android:paddingRight="5px"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/calib_Id"
            android:layout_width="50px"
            android:layout_height="36px"
            android:background="#BDC3BE"
            android:gravity="center"
            android:text=""
            android:textSize="20px"/>

        <TextView
            android:id="@+id/calib_Time"
            android:layout_width="220px"
            android:layout_height="36px"
            android:layout_marginLeft="10px"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20px"/>

        <TextView
            android:id="@+id/calib_Comp"
            android:layout_width="100px"
            android:layout_height="36px"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20px"/>

        <TextView
            android:id="@+id/calib_Range"
            android:layout_width="110px"
            android:layout_height="36px"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20px"/>

        /*隐藏了F，本来layout_width="100px"*/
        <TextView
            android:id="@+id/calib_K"
            android:layout_width="150px"
            android:layout_height="36px"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20px"/>

        /*隐藏了F，本来layout_width="100px"*/
        <TextView
            android:id="@+id/calib_B"
            android:layout_width="150px"
            android:layout_height="36px"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20px"/>

        <TextView
            android:id="@+id/calib_F"
            android:layout_width="100px"
            android:layout_height="36px"
            android:gravity="center_vertical"
            android:visibility="gone"
            android:text=""
            android:textSize="20px"/>

    </LinearLayout>

</LinearLayout>