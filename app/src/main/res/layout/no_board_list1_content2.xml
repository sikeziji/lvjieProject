<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/soft_bj"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="220px"

        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/layout_KBF_R1"
            android:layout_width="match_parent"
            android:layout_height="41px"
            android:layout_marginTop="50px"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/range_1"
                android:textColor="@android:color/white"
                android:textSize="25px" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40px"
                android:gravity="center_vertical"
                android:text="K"
                android:textColor="@android:color/white"
                android:textSize="25px" />

            <TextView
                android:id="@+id/tvRange1k"
                android:layout_width="120px"
                android:layout_height="41px"
                android:layout_marginLeft="10px"
                android:background="@drawable/valve_s"
                android:gravity="center"
                android:textAlignment="center"
                android:textColor="@android:color/white"
                android:textSize="25px" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:gravity="center_vertical"
                android:text="B"
                android:textColor="@android:color/white"
                android:textSize="25px" />

            <TextView
                android:id="@+id/tvRange1b"
                android:layout_width="120px"
                android:layout_height="41px"
                android:layout_marginLeft="10px"
                android:background="@drawable/valve_s"
                android:gravity="center"
                android:textAlignment="center"
                android:textColor="@android:color/white"
                android:textSize="25px" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20px"
                android:gravity="center_vertical"
                android:text="F"
                android:visibility="invisible"
                android:textColor="@android:color/white"
                android:textSize="25px" />

            <TextView
                android:id="@+id/tvRange1f"
                android:layout_width="120px"
                android:layout_height="41px"
                android:layout_marginLeft="10px"
                android:visibility="invisible"
                android:background="@drawable/valve_s"
                android:gravity="center"
                android:textAlignment="center"
                android:textColor="@android:color/white"
                android:textSize="25px" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_KBF_R2"
